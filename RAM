#RAM
#y=x^2

S=float(input('Start value of interval:'))
E=float(input('End value of interval:'))
N=float(input('number of rectangles:'))

H=float((E-S)/N)
print(H)

counter = 0

L=[]

while counter<E:
    
    y=float(((S+(counter*H))**2)*H)
    L.append(y)
    counter += H
    
print('LRAM:',sum(L))




#RRAM
